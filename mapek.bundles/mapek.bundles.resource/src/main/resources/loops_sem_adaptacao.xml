<loops>
	<loops>
	    <loop rate="1000">
			<sensors>
				<sensor className="br.com.vt.mapek.bundles.sensors.battery.IBatterySensor" />
			</sensors>
			<policys>
				<policy property="BATTERY" condition="ENTRE" lowerbound="0" upperbound="100"  />
			</policys>
			<actions>
			
				<action id="START_COMPONENT" params="rb_quicksort.bundle" />
			</actions>
		</loop>
	</loops>
</loops>
